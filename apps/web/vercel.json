{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": "nextjs",
  "cleanUrls": true,
  "trailingSlash": false,
  "buildCommand": "npm run build",
  "rewrites": [
    {
      "source": "/studio/(.*)",
      "destination": "https://dppadmin.sanity.studio/$1"
    }
  ]
}
